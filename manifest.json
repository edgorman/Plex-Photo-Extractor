{
  "manifest_version": 2,
  "name": "Plex Photo Extractor",
  "version": "0.1",
  "content_scripts": 
  [
    {
      "matches": [
        "https://app.plex.tv/*"
      ],
      "js": [
		"jquery-3.5.1.min.js", 
		"content.js"
	  ]
    }
  ],
  "background": {
    "scripts": [
	  "background.js"
    ]
  },
  "browser_action": {}
}